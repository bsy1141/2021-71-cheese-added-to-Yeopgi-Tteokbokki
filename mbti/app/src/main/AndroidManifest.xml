<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yubdduk.mbti">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mbti"
        android:usesCleartextTraffic="true">

        <activity android:name=".Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ReceipeActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".WillCookActivity"></activity>
        <activity android:name=".FlavorActivity"></activity>
        <activity android:name=".HungryActivity"></activity>
        <activity android:name=".YesterdayActivity"></activity>
        <activity android:name=".WillNotCook_ResultActivity"></activity>
        <activity android:name=".NationActivity"></activity>
        <activity android:name=".TimeActivity"></activity>
        <activity android:name=".LevelActivity"></activity>


    </application>

</manifest>